{
  "name": "api-graffiticode",
  "version": "0.0.1",
  "description": "Gateway for compilers as a service.",
  "author": "Art Compiler LLC",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/graffiticode/api"
  },
  "dependencies": {
    "aws-serverless-express": "^3.3.6",
    "body-parser": "^1.19.0",
    "errorhandler": "latest",
    "express": "^4.16.4",
    "hashids": "1.1.1",
    "method-override": "latest",
    "morgan": "latest",
    "redis": "2.6.3"
  },
  "scripts": {
    "start": "node app.js",
    "test": "mocha --reporter spec --recursive",
    "coverage": "nyc npm run test",
    "build": "tsc",
    "lambda:zip": "zip -r function.zip . -x=.git/*",
    "lambda:update": "aws lambda update-function-code --function-name api --zip-file fileb://function.zip",
    "deploy:lambda": "npm run build && npm run lambda:zip && npm run lambda:update"
  },
  "devDependencies": {
    "@types/node": "^12.0.2",
    "chai": "^4.2.0",
    "graceful-fs": "^4.1.15",
    "mocha": "^6.1.3",
    "nyc": "^14.1.1",
    "react": "0.14.5",
    "react-dom": "0.14.5",
    "sinon": "^7.4.1",
    "supertest": "^4.0.2",
    "typescript": "^3.5.2",
    "uglify-js": "^2.7.3"
  }
}
